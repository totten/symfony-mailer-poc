{
    "chmod": "0644",
    "check-requirements": false,
    "exclude-composer-files": false,
    "files": [
        "composer.json",
        "composer.lock",
        "pathload.json"
    ],
    "directories": [
        "vendor/composer"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["phpunit", "Tests", "Test", "tests", "test"],
            "in": "vendor"
        }
    ],
    "compactors": ["KevinGH\\Box\\Compactor\\Php", "KevinGH\\Box\\Compactor\\PhpScoper"],
    "git-version": "package_version",
    "main": false,
    "output": "vendor.phar",
    "shebang": false,
    "stub": true
}
